{
    "version": 2,
    "regions": ["sfo1", "cle1", "pdx1", "iad1"],
    "functions": {
        "api/**": {
            "memory": 3008,
            "maxDuration": 180
        }
    },
    "routes": [
        { "handle": "filesystem" },
        { "src": "/(.*)", "dest": "/api/index.ts" }
    ],
    "env": {
        "NODE_ENV": "production",
        "REDIS_HOST": "@packagephobia2-redis-host",
        "REDIS_PORT": "@packagephobia2-redis-port",
        "REDIS_PASS": "@packagephobia2-redis-pass",
        "GA_ID": "@packagephobia-ga-id"
    }
}
